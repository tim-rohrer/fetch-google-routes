{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "baseUrl": "src",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "outDir": "dist",
      "sourceMap": true
    },
    // "files": [
