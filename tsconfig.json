{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "baseUrl": "src",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "outDir": "dist",
      "sourceMap": true
    },
    "include": [
      "src/**/*.ts", "src/googleData.fixture"
    ],
    "exclude": [
      "node_modules",
      "typings",
      "src/**/__mocks__",
      "src/tests"
    ]
  }
